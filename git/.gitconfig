[user]
	name = smallkirby
	email = ssmallkirby@gmail.com
[push]
  defalt = matching
  autoSetupRemote = true
[init]
	defaultBranch = master
[alias]
  #commit = commit -s -S
  commit = commit
  st = status --short --branch --untracked-files=all
  unstage = restore --staged
  sw = switch
  ch = switch
  d = diff
  hist = log -p
  sync = fetch --all

[core]
  pager = less

[pager]
  show = vimpager
